<?xml version="1.0" encoding="UTF-8"?>

<!-- False positive Filter for FindBugs -->

<FindBugsFilter>
  
  <!-- automatically generated or external code that we can't influence -->
  <Match>
    <Or>
      <Class name="annis.visualizers.htmlvis.HTMLVisConfigLexer"/>
      <Class name="annis.visualizers.htmlvis.HTMLVisConfigParser"/>
    </Or>
  </Match>
  
  <Match>
    <Bug category="EXPERIMENTAL" />
  </Match>
  <Match>
    <Bug pattern="SE_NO_SERIALVERSIONID" />
  </Match>
</FindBugsFilter>
